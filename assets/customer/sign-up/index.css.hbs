.sl-input{
  align-items:center;
  background-color:rgba(var(--color-page-background));
  box-sizing:border-box;
  color:rgba(var(--color-text));
  display:flex;
  flex-flow:row nowrap;
  font-size:14px;
  justify-content:space-between;
  line-height:1.4;
  position:relative;
  transition:all .3s;
  width:100%;
}
.sl-input:after{
  border-color:rgba(var(--color-entry-line));
  border-style:solid;
  border-width:1px;
  bottom:0;
  content:"";
  left:0;
  pointer-events:none;
  position:absolute;
  right:0;
  top:0;
  transition:border-color .3s;
  z-index:1;
}
