.sl-card{
  border:1px solid rgba(var(--color-entry-line));
  font-size:14px;
  line-height:1.4;
  padding:0 40px 24px;
  word-break:break-word;
}
@media (max-width:767.98px){
  .sl-card{
    font-size:14px;
    padding:0 16px 16px;
  }
}
.sl-card [data-show=edit]{
  display:none;
}
.sl-card .btn{
  height:36px;
  letter-spacing:calc(var(--type-body-letter-spacing) / 1000);
  line-height:34px;
  max-width:192px;
  min-width:104px;
  overflow:hidden;
  padding:0 12px;
  text-overflow:ellipsis;
  white-space:nowrap;
}
@media (max-width:767.98px){
  .sl-card .btn{
    height:33px;
    line-height:31px;
    max-width:128px;
    min-width:80px;
  }
}
.sl-card__head{
  align-items:center;
  border-bottom:1px solid rgba(var(--color-entry-line));
  display:flex;
  flex-wrap:wrap;
  gap:12px 12px;
  padding:24px 0;
}
@media (max-width:767.98px){
  .sl-card__head{
    padding:16px 0 20px;
  }
}
.sl-card__head--context{
  flex:1;
}
.sl-card__head--context h2{
  font-size:20px;
  margin:0;
}
@media (max-width:767.98px){
  .sl-card__head--context h2{
    font-size:16px;
  }
}
.sl-card__head--context p{
  color:rgba(var(--color-text), .6);
  margin-top:4px;
}
@media (max-width:767.98px){
  .sl-card__head--context p{
    font-size:12px;
  }
}
.sl-card__head--edit-icon{
  cursor:pointer;
  display:flex;
}
@media (max-width:767.98px){
  .sl-card__head--edit-icon svg{
    height:20px;
    width:20px;
  }
}
.sl-card__content{
  padding:24px 0 16px;
}
@media (max-width:767.98px){
  .sl-card__content{
    padding:20px 0 0;
  }
}
.sl-card__footer{
  border-top:1px solid rgba(var(--color-entry-line));
  margin-top:8px;
  padding:24px 0 0;
}
@media (max-width:767.98px){
  .sl-card__footer{
    margin-top:20px;
    padding:20px 0 0;
  }
}
.sl-card__footer--buttons{
  align-self:flex-end;
  display:flex;
  justify-content:flex-end;
}
.sl-card__footer--buttons .btn+.btn{
  margin-left:12px;
}
@media (max-width:767.98px){
  .sl-card__footer--buttons .btn+.btn{
    margin-bottom:12px;
    margin-left:0;
  }
  .sl-card__footer--buttons{
    flex:1;
    flex-direction:column-reverse;
  }
  .sl-card__footer--buttons .btn{
    max-width:none;
  }
}

.sl-radio{
  box-sizing:border-box;
  cursor:pointer;
  line-height:1;
  list-style:none;
  margin:0;
  outline:none;
  padding:0;
  position:relative;
  top:.2em;
  white-space:nowrap;
}
.sl-radio+span{
  padding-left:8px;
  padding-right:8px;
}
.sl-radio__wrapper{
  font-feature-settings:"tnum";
  align-items:baseline;
  box-sizing:border-box;
  display:inline-flex;
  line-height:unset;
  list-style:none;
}
.sl-radio__input,.sl-radio__wrapper{
  cursor:pointer;
  margin:0;
  padding:0;
}
.sl-radio__input{
  height:100%;
  inset:0;
  opacity:0;
  position:absolute;
  width:100%;
  z-index:1;
}
.sl-radio__input:checked+.sl-radio__inner{
  border-color:rgba(var(--color-btn-background));
}
.sl-radio__input:checked+.sl-radio__inner:after{
  opacity:1;
  transform:scale(.5);
}
.sl-radio__inner{
  background-color:rgba(var(--color-page-background));
  border:1px solid rgba(var(--color-entry-line));
  border-collapse:separate;
  border-radius:50%;
  direction:ltr;
  left:0;
  position:relative;
  top:0;
  transition:all .3s;
}
.sl-radio__inner,.sl-radio__inner:after{
  display:block;
  height:16px;
  width:16px;
}
.sl-radio__inner:after{
  background-color:rgba(var(--color-btn-background));
  border-left:0;
  border-radius:16px;
  border-top:0;
  content:" ";
  left:50%;
  margin-left:-8px;
  margin-top:-8px;
  opacity:0;
  position:absolute;
  top:50%;
  transform:scale(0);
  transition:all .3s cubic-bezier(.78, .14, .15, .86);
}

