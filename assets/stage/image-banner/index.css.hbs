input{
  -webkit-appearance:none;
}
.stage-image-banner{
  overflow:hidden;
  position:relative;
}
.stage-image-banner-default-image{
  height:100%;
  width:100%;
}
.stage-image-banner.hidden{
  display:none;
}
.stage-image-banner .mask{
  background-color:#000;
  height:100%;
  left:0;
  opacity:.3;
  position:absolute;
  top:0;
  width:100%;
  z-index:1;
}
@media (min-width:750px){
  .stage-image-banner .default-image{
    height:650px;
  }
}
