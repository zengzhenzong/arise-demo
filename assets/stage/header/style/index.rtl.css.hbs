    stroke-dashoffset:0;
  }
  50%{
    stroke-dasharray:90, 150;
    stroke-dashoffset:-40;
  }
  to{
    stroke-dasharray:90, 150;
    stroke-dashoffset:-120;
  }
}
@keyframes mp-rotate{
  0%{
    transform:rotate(0deg);
  }
  to{
    transform:rotate(1turn);
  }
}
