@keyframes circleTransform{
  0%{
    stroke-dasharray:120;
    stroke-dashoffset:120;
  }
  to{
    stroke-dasharray:120;
    stroke-dashoffset:0;
  }
}
@keyframes hookmarkTransform{
  0%{
    stroke-dasharray:100 100;
    stroke-dashoffset:100;
  }
  to{
    stroke-dasharray:100 100;
    stroke-dashoffset:0;
  }
}
@keyframes mp-circular{
  0%{
